{
  "name": "mediaurl-ard-mediathek",
  "version": "0.0.0",
  "main": "dist",
  "scripts": {
    "build": "tsc",
    "start": "node .",
    "develop": "ts-node-dev --transpileOnly --no-notify src"
  },
  "dependencies": {
    "@mediaurl/sdk": "^1.0.1",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/node-fetch": "2.5.7",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
